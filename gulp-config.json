{
    "paths": {
        "sass": {
            "src": "app/scss/**/*.scss",
            "init": "app/scss/init.scss",
            "css": "app/css"
        },
        "scripts": {
            "src": "app/js/**/*.js",
            "dest": "app/js"
        },
        "images": {
            "src": "app/images",
            "build": "dist/images"
        }
    },
    "browserSupport": [
        "last 2 version",
        "safari 5",
        "ie 8",
        "ie 9",
        "opera 12.1",
        "ios 6",
        "android 4"
    ],
    "imageSettings": {
        "optimizationLevel": 5,
        "progressive": true,
        "interlaced": true
    }
}